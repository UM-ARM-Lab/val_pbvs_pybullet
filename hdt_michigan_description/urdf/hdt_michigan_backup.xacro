        </inertial>

    </link>

    <!-- Right EE 2 -->
    <!-- THIS IS THE SECOND PINCER, NEEDS TO MOVE EQUAL BUT OPPOSITE AS PINCER 1 -->
    <joint name="rightgripper2" type="revolute">
        <hdt id="10b" kmin="75" kmax="150" inertia="0.075"/>
        <axis xyz="0 0 1"/>
        <origin xyz="0 -0.0135 0.068553" rpy="${M_PI/2} -0.42 ${M_PI/2}"/>
        <parent link="end_effector_right"/>
        <child link="rightgripper2_link"/>
        <calibration rising="0.0"/>
        <dynamics damping="${gripper_joint_damping}" friction="0"/>
        <limit effort="35.0" lower="-0.116" upper="1" velocity="0.5"/>
    </joint>
    <xacro:hdt_transmission hardware_interface="${hardware_interface}" joint_name="rightgripper2"/>


    <link name="rightgripper2_link">
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://hdt_michigan_description/meshes/PINCER_FINGER.STL" scale="1.0 1.0 1.0"/>
            </geometry>
        </collision>

        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://hdt_michigan_description/meshes/PINCER_FINGER.STL"/>
            </geometry>
            <material name="blue"/>
        </visual>

        <inertial>
            <origin xyz="0.034 0.055 0.007" rpy="0 0 0"/>
            <mass value="0.06821764"/>
            <inertia ixx="0.00007499"
                     ixy="0"
                     ixz="0"
                     iyy="0.00002842"
                     iyz="0"
                     izz="0.00010118"/>
        </inertial>

    </link>

    <!-- NOTE: defining tools here allows moveit to plan with respect to these frames very easily.
      I think it makes sense to leave this file un-modified on shared accounts, and overwrite the robot description
      on the parameter server from your local modified copy on a personal account. -->
    <link name="left_tool">
        <visual name="left_tool_visual">
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.002"/>
            </geometry>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.001"/>
            <inertia ixx="0"
                     ixy="0"
                     ixz="0"
                     iyy="0"
                     iyz="0"
                     izz="0"/>
        </inertial>
    </link>
    <joint name="left_tool_joint" type="fixed">
        <parent link="end_effector_left"/>
        <child link="left_tool"/>
        <origin xyz="0.0 0.0 0.181" rpy="0 0 0"/>
    </joint>

    <link name="right_tool">
        <visual name="right_tool_visual">
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.002"/>
            </geometry>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.001"/>
            <inertia ixx="0"
                     ixy="0"
                     ixz="0"
                     iyy="0"
                     iyz="0"
                     izz="0"/>
        </inertial>
    </link>
    <joint name="right_tool_joint" type="fixed">
        <parent link="end_effector_right"/>
        <child link="right_tool"/>
        <origin xyz="0.0 0.0 0.181" rpy="0 0 0"/>
    </joint>
    <xacro:include filename="$(find hdt_michigan_description)/urdf/gazebo_utilities.xacro"/>

    <xacro:add_gazebo_control_plugin robot_namespace="${robot_name}"/>
    <xacro:add_moveit_planning_scene_plugin robot_namespace="${robot_name}" root_frame_id="${robot_name}_root"/>

    <xacro:add_gazebo_fixed_link base_link="${robot_name}_root"/>
</robot>

